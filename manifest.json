{
  "manifest_version": 2,
  "name": "Zoho Helper",
  "version": "0.0.0",
  "permissions": [],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Zoho Helper",
    "default_icon": {
      "32": "z-blue.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://inventory.zoho.eu/*"
      ],
      "css": [
        "content.css"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}